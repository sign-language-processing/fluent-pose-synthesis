{
    "seed": 1024,
    "arch": {
        "decoder": "trans_enc",
        "latent_dim": 256,
        "ff_size": 1024,
        "num_heads": 4,
        "num_layers": 4,
        "chunk_len": 40,
        "keypoints": 178,
        "dims": 3,
        "dropout": 0.2,
        "activation": "gelu",
        "ablation": null,
        "legacy": false,
        "history_len": 10
    },
    "diff": {
        "noise_schedule": "cosine",
        "diffusion_steps": 8,
        "sigma_small": true
    },
    "trainer": {
        "epoch": 500,
        "lr": 1e-4,
        "batch_size": 1024,
        "cond_mask_prob": 0.15,
        "use_loss_mse": true,
        "use_loss_vel": true,
        "use_loss_accel": false,
        "lambda_vel": 0,
        "lambda_accel": 0,
        "guidance_scale": 2.0,
        "workers": 4,
        "load_num": -1,
        "validation_chunk_size": 40,
        "validation_stop_threshold": 1e-4,
        "eval_freq": 1,
        "use_amp": false
    }
}